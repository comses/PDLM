<?xml version="1.0" ?>
<statechart:Model xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:statechart="http://statechart/1.0" xmi:version="2.0">
  <initialState Name="start" Description="" outgoing="//@transitions.0"/>
  <states xsi:type="statechart:State" Name="sendingSignal">
    <outputFunctions OutputPort="signal_out">
      <Message>signal_out</Message>
      <Action>Toggle signal state</Action>
    </outputFunctions>
  </states>
  <transitions xsi:type="statechart:Transition" source="//@initialState" target="//@states.0">
    <Action>Set time_advance to frequency</Action>
  </transitions>
  <transitions xsi:type="statechart:InternalTransition" TimeAdvanceType="Value" TimeAdvanceValue="2.0" source="//@states.0" target="//@states.0">
    <Action>Set time_advance to frequency</Action>
    <Guard/>
  </transitions>
</statechart:Model>
