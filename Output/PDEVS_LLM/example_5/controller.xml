<?xml version="1.0" ?>
<statechart:Model xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:statechart="http://statechart/1.0" xmi:version="2.0">
  <initialState Name="start" Description="" outgoing="//@transitions.0"/>
  <states xsi:type="statechart:State" Name="inactive"/>
  <states xsi:type="statechart:State" Name="active">
    <outputFunctions OutputPort="control_out">
      <Message>control_out</Message>
      <Action>toggleSignal()</Action>
    </outputFunctions>
  </states>
  <transitions xsi:type="statechart:Transition" source="//@initialState" target="//@states.0">
    <Action>time_advance=10</Action>
  </transitions>
  <transitions xsi:type="statechart:InternalTransition" TimeAdvanceType="Value" TimeAdvanceValue="1.0" source="//@states.0" target="//@states.1">
    <Action>state='active'; time_advance=1</Action>
    <Guard/>
  </transitions>
  <transitions xsi:type="statechart:InternalTransition" TimeAdvanceType="Value" TimeAdvanceValue="1.0" source="//@states.1" target="//@states.1">
    <Action>toggleSignal()</Action>
    <Guard/>
  </transitions>
</statechart:Model>
