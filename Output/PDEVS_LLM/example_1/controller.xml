<?xml version="1.0" ?>
<statechart:Model xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:statechart="http://statechart/1.0" xmi:version="2.0">
  <initialState Name="inactive" Description="" outgoing="//@transitions.0"/>
  <states xsi:type="statechart:State" Name="active">
    <outputFunctions OutputPort="signal_out">
      <Message>on</Message>
      <Action>Toggle output signal</Action>
    </outputFunctions>
  </states>
  <transitions xsi:type="statechart:Transition" source="//@initialState" target="//@states.0">
    <Action>Set time_advance to 1</Action>
  </transitions>
  <transitions xsi:type="statechart:InternalTransition" TimeAdvanceType="Value" TimeAdvanceValue="1.0" source="//@states.0" target="//@states.0">
    <Action>Toggle state and reset time_advance</Action>
    <Guard/>
  </transitions>
</statechart:Model>
