<?xml version="1.0" ?>
<statechart:Model xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:statechart="http://statechart/1.0" xmi:version="2.0">
  <initialState Name="start" Description="" outgoing="//@transitions.0"/>
  <states xsi:type="statechart:State" Name="off">
    <outputFunctions OutputPort="light_status_out">
      <Message>off</Message>
      <Action>send(state)</Action>
    </outputFunctions>
  </states>
  <states xsi:type="statechart:State" Name="on">
    <outputFunctions OutputPort="light_status_out">
      <Message>on</Message>
      <Action>send(state)</Action>
    </outputFunctions>
  </states>
  <transitions xsi:type="statechart:Transition" source="//@initialState" target="//@states.0">
    <Action>time_advance=1</Action>
    <Action>state=off</Action>
  </transitions>
  <transitions xsi:type="statechart:InternalTransition" TimeAdvanceType="Value" TimeAdvanceValue="1.0" source="//@states.1" target="//@states.0">
    <Action>toggle(state)</Action>
    <Action>time_advance=1</Action>
    <Guard/>
  </transitions>
  <transitions xsi:type="statechart:InternalTransition" TimeAdvanceType="Value" TimeAdvanceValue="1.0" source="//@states.0" target="//@states.1">
    <Action>toggle(state)</Action>
    <Action>time_advance=1</Action>
    <Guard/>
  </transitions>
  <transitions xsi:type="statechart:ExternalTransition" TimeAdvanceType="Value" TimeAdvanceValue="1.0" source="//@states.0" target="//@states.1" InputPort="control_in">
    <Action>toggle(state)</Action>
    <Action>time_advance=1</Action>
    <Guard/>
  </transitions>
  <transitions xsi:type="statechart:ExternalTransition" TimeAdvanceType="Value" TimeAdvanceValue="1.0" source="//@states.1" target="//@states.0" InputPort="control_in">
    <Action>toggle(state)</Action>
    <Action>time_advance=1</Action>
    <Guard/>
  </transitions>
</statechart:Model>
